<%- include("templates/header") %> 
  <h2>Signup:</h2>
    <form action='/submitSignup' method='POST'>
      <div class="form-floating">
        <input id="floatUser" class="form-control" type='username' name='username' placeholder='Username'/>
        <label for="floatUser">Username</label>
      </div>
      <div class="form-floating">
        <input id="floatingInput" class="form-control" type='email' name='email' placeholder='Email'/>
        <label for="floatingInput">Email address</label>
      </div>
      <div class="form-floating">
        <input id="floatingPassword" class="form-control" type='password' name='password' placeholder='Email'/>
        <label for="floatingPassword">Password</label>
      </div>
      <button class="w-100 btn btn-lg btn-primary">Submit</button>
    </form>
<%- include("templates/footer") %> 